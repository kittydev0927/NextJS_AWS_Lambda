{
  "name": "cpp-ui-lib",
  "version": "2.4.3504",
  "description": "NextJS CPP App and Library of React/TS UI components for the Customer Portal Platform",
  "main": "index.ts",
  "types": "dist/typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/progressdevelopmentteam/cpp-ui.git"
  },
  "keywords": [],
  "author": "engineering@progressresidential.com",
  "license": "ISC",
  "homepage": "https://bitbucket.org/progressdevelopmentteam/cpp-ui#readme",
  "scripts": {
    "build:storybook": "build-storybook",
    "build:types": "tsc -p tsconfig.build.json",
    "build": "next build",
    "clean": "rimraf ./.next ./out ./obj ./artifacts ./**/tsconfig.tsbuildinfo",
    "co:login": "aws codeartifact login --tool npm --repository pr-nodejs --domain rentprogress --domain-owner 237840265779 --region us-west-2",
    "cypress:artifacts": "mkdir -p .next artifacts/cypress cypress",
    "cypress:ui": "HOST_UID=$(id -u) HOST_GID=$(id -g) docker compose run --rm -e CYPRESS_BASE_URL=http://host.docker.internal:3000 cypress open",
    "dev": "next dev -p 3002",
    "format": "prettier --write .",
    "lint": "eslint --report-unused-disable-directives \"**/*.{js,ts,jsx,tsx}\"",
    "lint:css": "stylelint \"**/*.{css,scss}\"",
    "lint:prettier": "prettier --check .",
    "lint:ts": "eslint --report-unused-disable-directives --cache --cache-location ./obj/.eslintcache \"**/*.{js,jsx,ts,tsx}\"",
    "lint-staged": "lint-staged",
    "precypress:ui": "yarn cypress:artifacts",
    "prelint:ts": "tsc --build ./tsconfig.json",
    "prepare": "husky install",
    "prepublish": "yarn build:types",
    "pretest:integration": "yarn cypress:artifacts",
    "snapshots": "percy storybook ./storybook-static",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "test": "cross-env TS_JEST_DISABLE_VER_CHECKER=true node --expose-gc node_modules/jest/bin/jest.js --runInBand --logHeapUsage --coverage components",
    "test:integration": "HOST_UID=$(id -u) HOST_GID=$(id -g) docker compose --profile integration up --build --abort-on-container-exit --exit-code-from cypress"
  },
